{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": "^7.4||^8.0",
    "artesaos/seotools": "0.*",
    "aws/aws-sdk-php": "^3.0",
    "doctrine/dbal": "^2.9",
    "fideloper/proxy": "^4.2",
    "fruitcake/laravel-cors": "^2.0",
    "guzzlehttp/guzzle": "^7.0.1",
    "laravel/fortify": "^1.6",
    "laravel/framework": "^8.0",
    "laravel/horizon": "^5.0",
    "laravel/legacy-factories": "^1.0",
    "laravel/tinker": "^2.0",
    "laravelium/sitemap": "^8.0",
    "league/csv": "^9.0",
    "livewire/livewire": "^2.4",
    "revolution/laravel-mastodon-api": "^2.0",
    "revolution/laravel-namespaced-helpers": "^1.0",
    "revolution/socialite-mastodon": "^1.0",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "facade/ignition": "^2.3.6",
    "fakerphp/faker": "^1.9.1",
    "laravel/sail": "^1.0.1",
    "mockery/mockery": "^1.3.1",
    "nunomaduro/collision": "^5.0",
    "itsgoingd/clockwork": "^5.0",
    "barryvdh/laravel-ide-helper": "^2.4",
    "phpunit/phpunit": "^9.3"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover --ansi"
    ],
    "ide-helper:meta": "php artisan ide-helper:meta",
    "ide-helper:generate": "php artisan ide-helper:generate",
    "ide-helper:models": "php artisan ide-helper:models -N",
    "sail:up": "vendor/bin/sail up -d",
    "sail:down": "vendor/bin/sail down"
  }
}
