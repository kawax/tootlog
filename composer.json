{
  "name": "laravel/laravel",
  "description": "The Laravel Framework.",
  "keywords": [
    "framework",
    "laravel"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=5.6.4",
    "aws/aws-sdk-php": "~3.0",
    "barryvdh/laravel-debugbar": "^2.3",
    "barryvdh/laravel-ide-helper": "^2.3",
    "cakephp/chronos": "^1.1",
    "christofferok/laravel-emojione": "^2.0",
    "doctrine/dbal": "~2.3",
    "laravel/framework": "5.4.*",
    "laravel/tinker": "~1.0",
    "laravelcollective/html": "^5.4",
    "mccool/laravel-auto-presenter": "^5.0",
    "peppeocchi/laravel-elb-middleware": "^1.0",
    "revolution/laravel-mastodon-api": "^1.0",
    "revolution/socialite-mastodon": "^1.0",
    "roumen/sitemap": "^2.6",
    "torann/json-ld": "^0.0.9",
    "usmanhalalit/laracsv": "dev-master"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "laravel/homestead": "^5.2",
    "mockery/mockery": "0.9.*",
    "phpunit/phpunit": "~5.7",
    "pda/pheanstalk": "^3.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
