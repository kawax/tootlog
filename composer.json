{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0.2",
        "aws/aws-sdk-php": "^3.212",
        "biscolab/laravel-recaptcha": "^5.3",
        "doctrine/dbal": "^3.3",
        "guzzlehttp/guzzle": "^7.4",
        "laravel/fortify": "^1.11",
        "laravel/framework": "^9.2",
        "laravel/horizon": "^5.9",
        "laravel/tinker": "^2.7",
        "league/csv": "^9.8",
        "livewire/livewire": "^2.10",
        "revolution/laravel-mastodon-api": "^3.0",
        "revolution/laravel-namespaced-helpers": "^1.3",
        "revolution/socialite-mastodon": "^1.3",
        "spatie/laravel-sitemap": "^6.1",
        "symfony/http-client": "^6.0",
        "symfony/mailgun-mailer": "^6.0"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.12",
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.0",
        "laravel/legacy-factories": "^1.3",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5",
        "revolution/sail-db-backup": "^1.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "ext-exif": "1.0.0"
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:generate"
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "ide-helper:meta": "php artisan ide-helper:meta",
        "ide-helper:generate": "php artisan ide-helper:generate",
        "sail:up": "vendor/bin/sail up -d",
        "sail:down": "vendor/bin/sail down",
        "sail:backup": "vendor/bin/sail art sail:backup:mysql"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
